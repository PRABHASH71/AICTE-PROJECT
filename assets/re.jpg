.oda-chat-header {
    align-items: center;
    box-sizing: border-box;
    color: #fff;
    display: flex;
    flex-direction: row;
    height: 48px;
    padding: 0 8px 0 10px;
    text-align: start;
}

.oda-chat-logo {
    flex: 0 0 auto;
    height: 24px;
    max-height: 32px;
    max-width: 32px;
    min-height: 16px;
    padding: 0;
    width: 24px;
}

.oda-chat-header-info-wrapper {
    align-items: flex-start;
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    margin: 0 8px;
    min-width: 0;
    padding: 0;
    width: 100%;
}

.oda-chat-title {
    color: #fff;
    font-size: 17px;
}

.oda-chat-subtitle {
    color: #fff;
    font-size: 14px;
}

.oda-chat-connection-status {
    font-size: 10px;
    font-weight: 700;
    text-transform: uppercase;
}

.oda-chat-title, .oda-chat-subtitle, .oda-chat-connection-status {
    justify-content: center;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.oda-chat-header-custom-element {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
}

.oda-chat-header-actions {
    display: flex;
    flex: 1 0 auto;
    justify-content: flex-end;
}

.oda-chat-header-button {
    align-items: center;
    border: none;
    border-radius: 2px;
    cursor: pointer;
    display: flex;
    height: 32px;
    justify-content: center;
    margin: 0 0 0 8px;
    max-height: 32px;
    max-width: 32px;
    padding: 4px;
    width: 32px;
}

.oda-chat-header-button:disabled {
    cursor: default;
    pointer-events: none;
}

.oda-chat-header-button svg {
