    max-height: 100%;
    max-width: 100%;
    width: 100%;
}

.oda-chat-header-button:disabled .oda-chat-header-button-icon,
.oda-chat-header-button:disabled .oda-chat-header-button-icon:active,
.oda-chat-header-button:disabled .oda-chat-header-button-icon:hover {
    fill: #bbb6b2;
}

